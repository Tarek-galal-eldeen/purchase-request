<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <!--sequences for hospital.patient-->
        <record id="seq_hospital_patient" model="ir.sequence">
            <field name="name">Patient Sequence</field>
            <field name="code">patient.sequence</field>
            <field name="prefix">HP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!--sequences for hospital.appointments-->
        <record id="seq_hospital_appointments" model="ir.sequence">
            <field name="name">Appointment Sequence</field>
            <field name="code">appointment.sequence</field>
            <field name="prefix">OT</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!--sequences for hospital.doctors-->
        <record id="seq_hospital_doctors" model="ir.sequence">
            <field name="name">Doctors Sequence</field>
            <field name="code">doctor.sequence</field>
            <field name="prefix">DR</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>